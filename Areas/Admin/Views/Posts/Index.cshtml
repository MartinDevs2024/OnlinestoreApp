@model IEnumerable<Post>

@{
    var title = "Post";
}
<div id="wrapper">
    <partial name="_AdminSideNavbar" />
    <div id="content-wrapper" class="d-flex flex-column">
        <div id="content">
            <!-- Topbar -->
            <!-- Topbar -->
            <partial name="_AdminTopBar" />
        <div class="container-fluid">
            <div class="text-center mt-3">
                <h2>@title</h2>
                <hr />
            </div>
            <div class="table-responsive p-2">
                <table id="mypost" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                        <tr>
                            <th>Title</th>
                            <th>Body</th>
                            <th>Image</th>
                            <th>Description</th>
                            <th>Category</th>
                            <th>Tags</th>
                            <th>Edit</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->
</div>
</div>
@section scripts {
    <script src="~/js/post.js"></script>
    <script src="~/js/menutoggle.js"></script>
}